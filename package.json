{
  "name": "@morizo/vue-ui",
  "version": "1.0.7",
  "description": "A Component Library for Vue 3",
  "keywords": [
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "homepage": "https://github.com/RayBob1993/morizo-vue/",
  "bugs": {
    "url": "https://github.com/RayBob1993/morizo-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RayBob1993/morizo-vue.git"
  },
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/morizo-vue.umd.cjs",
  "module": "./dist/morizo-vue.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/morizo-vue.js"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "20.16.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir ./dist/types",
    "types": "vue-tsc ",
    "lint:js": "eslint . --fix",
    "lint": "pnpm lint:js",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "inputmask": "5.0.9",
    "nouislider": "15.8.1",
    "@fancyapps/ui": "5.0.36",
    "async-validator": "4.2.5"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "@vue/test-utils": "2.4.6",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@types/node": "22.10.2",
    "vitest": "2.1.5",
    "happy-dom": "15.11.7",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@types/inputmask": "5.0.7",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "sass": "1.77.6",
    "vue-tsc": "2.1.10",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.31.0",
    "eslint-plugin-import": "2.31.0",
    "@vue/eslint-config-typescript": "13.0.0"
  }
}
